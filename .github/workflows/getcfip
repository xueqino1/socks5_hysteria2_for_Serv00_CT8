name: getcmliuip # yml文件名
 
on:
  push:
    branches:
      - main
  schedule:   
    - cron: '0 21 * * *'   # 定时运行的时间，国际标准时间，+8为对应的北京时间
 
jobs:
  run-coupons: # 任务名
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@main  #采用main分支下的
           
      - name: Setup Node.js
        uses: actions/setup-node@v2  #安装node.js
        with:
          node-version: '14'
          
      - name: Install Dependency       
        run: |
          #npm install
          #npm install parse    
          node ./getcmliuip.js  # 要运行的js脚本，放在根目录下
